#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules/@module-federation/enhanced/bin/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules/@module-federation/enhanced/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules/@module-federation/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules/@module-federation/enhanced/bin/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules/@module-federation/enhanced/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules/@module-federation/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/@module-federation+enhanced@0.20.0_@rspack+core@1.5.8_@swc+helpers@0.5.17__react-dom@19_b5f26abb3aab6ea485c457b0f691f78b/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/mf.js" "$@"
else
  exec node  "$basedir/../../bin/mf.js" "$@"
fi
