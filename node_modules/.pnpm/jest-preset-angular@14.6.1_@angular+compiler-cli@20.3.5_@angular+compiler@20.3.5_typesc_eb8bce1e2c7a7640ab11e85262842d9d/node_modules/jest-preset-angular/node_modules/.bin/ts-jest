#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/max/nabdchainsystem/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_2774d88ad462f9a5bd18d7936ce6fa5e/node_modules/ts-jest/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_2774d88ad462f9a5bd18d7936ce6fa5e/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/max/nabdchainsystem/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_2774d88ad462f9a5bd18d7936ce6fa5e/node_modules/ts-jest/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_2774d88ad462f9a5bd18d7936ce6fa5e/node_modules:/Users/max/nabdchainsystem/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../ts-jest/cli.js" "$@"
fi
